  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 파일명과 동일하게 -->
<mapper namespace="category">
	<!-- CategoryController -->
	<!-- 카테고리 List -->
	<select id="get_categoryList" resultType="mybatis.vo.CategoryVO">
